<div id="productbox">
  <TMPL_IF NAME="pageCount">
    <TMPL_INCLUDE NAME="pageNumbers.tmpl">

      <article class="itemrow">
        <b>Click on Image or Part Number for Detailed Product Information
          <TMPL_IF NAME="session_isGuest">
            <TMPL_ELSE>
              <p class="right1">Place in cart to check availability in Real Time
        </b></p>
  </TMPL_IF>
  <TMPL_LOOP NAME="products">
    <TMPL_IF NAME="changeValue">
      <tr>
        <td class="changedValue" colspan=4>
          <TMPL_VAR NAME="changeValue">
        </td>
      </tr>
    </TMPL_IF>

    <div class="itemtable"><!--start search result item -->

      <div class="thumbnail">
        <TMPL_IF NAME="filename"><a href="/product.html?productCode=<TMPL_VAR NAME=" ProductCode" escape=url>">
            <img src="<TMPL_VAR NAME=" glob_thumbnailPrefix">/<TMPL_VAR NAME="filename">" title="<TMPL_VAR
                NAME="ProductCode" escape=html> image" alt="image"></a></TMPL_IF>
      </div>
      <div class="chunk1">
        <p class="medium"><b><a href="/product.html?productCode=<TMPL_VAR NAME=" ProductCode" escape=url>"> <TMPL_VAR
                NAME="ProductCode"> </a></b>
          <TMPL_IF NAME="session_isSales">
            <TMPL_IF NAME="ShowPR"><a href="/pr.html?ProductCode=<TMPL_VAR NAME=" ProductCode">">P</a></TMPL_IF>
          </TMPL_IF>
        </p>

        <p class="medium">
          <TMPL_VAR NAME="ProductDescription">
        </p>
        <TMPL_VAR NAME="Memo1">
          <TMPL_VAR NAME="Cat_Description">
            <TMPL_IF NAME="SpecialOrder">
              <TMPL_ELSE>
                <TMPL_IF NAME="allowQtyAvailable">
                  <p class="smallChunk">Stock Availability: <TMPL_VAR NAME="QtyAvailable">
                  </p>
                </TMPL_IF>
                <p class="smallChunk">Carton Quantity: <TMPL_VAR NAME="PerBox">
                </p>
                <TMPL_IF NAME="StatusFlagN">
                  <p class="notAvailable">Product no longer available.</p>
                </TMPL_IF>
                <TMPL_IF NAME="session_isGuest">
                  <TMPL_ELSE>
                    <TMPL_IF NAME="LastOrderedDate">
                      <p class="smallChunk">Last Order Date: <TMPL_VAR NAME="LastOrderedDate">
                      </p>
                    </TMPL_IF>
                </TMPL_IF>
                <TMPL_IF NAME="session_isGuest">
                  <TMPL_ELSE>
                    <TMPL_IF NAME="QtyOrderedInPast">
                      <p class="smallChunk">Ordered in last 24 months: <TMPL_VAR NAME="QtyOrderedInPast">
                      </p>
                    </TMPL_IF>
                </TMPL_IF>
            </TMPL_IF>
      </div>
      <div class="chunk2">
        <TMPL_IF NAME="CatalogPageNo">
          <a href="/viewCatalog.html?page=<TMPL_VAR NAME=" CatalogPageNo">" target="_blank"><button
              class="newbutton1">Catalog Page</button></a>
        </TMPL_IF>
        <TMPL_IF NAME="youtubeCode">
          <a href="#" class="youtube-link" youtubeid="<TMPL_VAR NAME=" youtubeCode">"><button class="newbutton3"
              id="showProductVideo">Product Video</button></a>
        </TMPL_IF>
        <br />
        <TMPL_IF NAME="session_isGuest">
          <TMPL_ELSE>
            <TMPL_INCLUDE NAME="favoriteButton.tmpl">
        </TMPL_IF>
      </div>
      <div class="chunk3">
        <TMPL_IF NAME="SpecialOrder">
          <TMPL_ELSE>
            <TMPL_IF NAME="ladder">
              <TMPL_IF NAME="showSpecialsLabel">
                <div class="promo">* Special Price</div>
              </TMPL_IF>
              <table class="ladders">
                <TMPL_LOOP NAME="ladder">
                  <tr>
                    <td>
                      <TMPL_VAR NAME="range">
                    </td>
                    <td class="white-space: nowrap">
                      <span<TMPL_IF NAME="special"> class="promo"
            </TMPL_IF>><TMPL_VAR NAME="price"></span> / <TMPL_VAR NAME="UofM">
      </div>
</div>
</div>
  </TMPL_LOOP>
</div>
<TMPL_ELSE>
  <p class="medium">
    <TMPL_VAR NAME="your"> List Price $<TMPL_VAR NAME="Price"> / <TMPL_VAR NAME="UofM">
  </p>
  </TMPL_IF>

  <TMPL_IF NAME="session_isGuest">
    <TMPL_ELSE>
      <div>
        <input type="text" class="quantity addQuantity" placeholder="Quantity" value="<TMPL_VAR NAME=" Qty">"
        useProductCode="<TMPL_VAR NAME="productCode" escape=html>" ProductCode="<TMPL_VAR NAME="cleanProductCode">"
            perbox="<TMPL_VAR NAME="perBox" default=0>" allowpartialcases="<TMPL_VAR NAME="AllowPartialCases"
                DEFAULT="">">
                <button class="addProductToCart qty" useProductCode="<TMPL_VAR NAME=" productCode" escape=html>"
                  ProductCode="<TMPL_VAR NAME="ProductCode">" />
      </div>
      <TMPL_IF NAME="Qty">
        <TMPL_INCLUDE NAME="cartrow-template-addedToCart-shown.tmpl">
          <TMPL_ELSE>
            <TMPL_INCLUDE NAME="cartrow-template-addedToCart-hidden.tmpl">
      </TMPL_IF>
  </TMPL_IF>
  </TMPL_IF>

  <TMPL_IF NAME="showExplanation">
    <tr>
      <td colspan=4>
        <h2>Price Ladder Explanation for <TMPL_VAR NAME="ProductCode"> visible only to admins</h2>
        <TMPL_VAR NAME="explanation">
      </td>
    </tr>
  </TMPL_IF>
  </TMPL_LOOP>
  </div> <!-- end search result item-->
  </article>
  <TMPL_INCLUDE NAME="pageNumbers.tmpl">
    <TMPL_ELSE>
      <TMPL_IF NAME="prod_normal">
        <b>No items found in our catalog.</b>
      </TMPL_IF>
      <TMPL_IF NAME="prod_favorites">
        <b>No items found in your favorites.</b>
        <form method=post><input type=submit value="Click Here"><input type="hidden" name="productShowOnly"
            value="All"><input type="hidden" name="searchFor" value="<TMPL_VAR NAME=" searchFor" ESCAPE=HTML>"></form>
        to
        search our entire catalog.
      </TMPL_IF>
      <TMPL_IF NAME="prod_history">
        <b>No items found in your order history.</b>
        <form method=post><input type=submit value="Click Here"><input type="hidden" name="productShowOnly"
            value="All"><input type="hidden" name="searchFor" value="<TMPL_VAR NAME=" searchFor" ESCAPE=HTML>"></form>
        to
        search our entire catalog.
      </TMPL_IF>
      <TMPL_IF NAME="prod_shoplist">
        <b>No items found in your shopping list.</b>
        <form method=post><input type=submit value="Click Here"><input type="hidden" name="productShowOnly"
            value="All"><input type="hidden" name="searchFor" value="<TMPL_VAR NAME=" searchFor" ESCAPE=HTML>"></form>
        to
        search our entire catalog.
      </TMPL_IF>
      <br><br>
      <div>If you cannot find what you're looking for, please give us a call at 800-244-1455 so we can help!</div>
      </TMPL_IF>
      </div>
      <TMPL_INCLUDE NAME="youtube_code.tmpl">