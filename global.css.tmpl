/* Pico CSS */
:root {
    --primary: #296adb;
}

:root:not([data-theme=dark]),
[data-theme=light] {
    --primary: #296adb;
    --primary-hover: #1b4388;
}

/* Header Styles -----------------------------------------------------------------*/

html,
body {
    background: #dddddd;
    font-family: arial, sans-serif;
    font-size: 16px;
    margin: 0;
    padding: 0;
    height: 100%;
    width: 100%;
}

#wrap {
    max-width: 1100px;
    min-height: 100%;
    position: relative;
    margin: 0 auto;
    background: #f8f8f8;
    box-shadow: 0px 3px 5px rgba(0, 0, 0, 0.2);
}

.nowrap {
    white-space: nowrap;
}

#maincontent {
    padding-bottom: 80px;
}

#header {
    /* background: url('dist/header-bg.jpg');
    TODO wendy: new header background if picture prefered 
    */
    background: #124280;
    max-width: 1100px;
    margin: 0px 0px -10px 0px;
    display: grid;

    grid-template-columns: 3fr 1fr;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
}

@media (min-width: 100px) {
    #header {
        font-size: 0.8em;
        grid-template-rows: 120px auto;
    }
}

@media (min-width: 313px) {
    #header {
        font-size: 0.8em;
        grid-template-rows: 115px auto;
    }
}

@media (min-width: 495px) {
    #header {
        grid-template-rows: 110px auto;
    }
}

@media (min-width: 768px) {
    #header {
        font-size: 1em;
    }
}



.con-info {
    display: grid;
    min-height: 90px;
    /* to match contactbox*/
    grid-template-columns: auto;
    grid-template-rows: 75px auto;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
}

.header-logo {
    /* TODO wendy: new cool logo :)
    update 5/8: ty Wendy
     */
    margin: 0px -5px;
    float: left;
    min-height: 40px;
    max-height: 75px;
    grid-area: 1 / 1 / 2 / 2;

}

.header-info {
    font-size: 0.9em;
    color: #ececec;
    padding: 0px 0px 0px 20px;
    grid-area: 2 / 1 / 3 / 2;
}

.header-title {
    margin: 0px;
    color: #ececec;
    font-weight: bold;
    font-size: 0.9em;
    max-height: 25px;
}

.header-subtitle {
    margin: 0px;
    color: #ececec;
    font-size: min(3.5vw, 2.5vh);
    margin: 0px;
    max-height: 20px;
}

.contactbox {
    border-radius: 3px;
    min-width: 90px;
    min-height: 90px;
    margin: 10px 10px 10px auto;
    text-align: right;
}

@media (min-width: 720px) {
    .contactbox {
        grid-area: 1 / 2 / 2 / 3;
        padding: 15px;
    }
}


.contactbox p a {
    font-size: 12t;
    margin: 5px 5px 0px 5px;
    color: #ececec;
    text-shadow: 0px 0px 2px #111111;
    text-decoration: none;
}

.contactbox p a:visited {
    text-decoration: none;
    color: #ececec;
    text-shadow: 0px 0px 2px #111111;
}

.contactbox p a:hover {
    text-decoration: none;
    color: #ececec;
    text-shadow: 0px 0px 2px #111111;
}


p.update {
    font-size: 9pt;
    margin: 10px 5px 8px 5px;
    color: #cc0000;
}

/* Search Bar 
Button 
and Cart


*/


.search-wrap {
    border-radius: 4px;
    display: grid;
    grid-template-columns: auto 65px;
    grid-template-rows: 50px;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
    margin: 0px -5px;

}

form.searchbar {
    grid-area: 1 / 1 / 2 / 2;
    display: grid;
    height: 50px;
    margin: 0px 10px 0px 10px;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
    grid-template-rows: inherit;
}

form.searchbar input[type=search] {
    box-shadow: 0 0 3px #aba8a8;
    box-sizing: border-box;
    position: relative;
    left: -10px;

}

form.searchbar input[type=text]:focus {
    outline: 0;
}

form.searchbar button {
    border: 0px;
    grid-area: 1 / 2 / 2 / 3;
    height: inherit;

}

/* search button resize */
@media (min-width: 100px) {
    form.searchbar {
        grid-template-columns: auto 60px;
    }

    form.searchbar button {
        font-size: 0;
    }

    button i.fa-solid {
        font-size: 15pt;
        padding: 2.5pt;
    }
}

@media (min-width: 768px) {
    form.searchbar {
        grid-template-columns: auto 120px;
    }

    form.searchbar button.button {
        font-size: 0.9em;
    }

    button i.fa-solid {
        font-size: 12pt;
    }
}

/*end button resize*/

