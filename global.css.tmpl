        hsla(0, 0%, 0%, 0.228) 44.1%,
        hsla(0, 0%, 0%, 0.169) 49.6%,
        hsla(0, 0%, 0%, 0.115) 56.3%,
        hsla(0, 0%, 0%, 0.069) 64.4%,
        hsla(0, 0%, 0%, 0.033) 74.2%,
        hsla(0, 0%, 0%, 0.009) 86%,
