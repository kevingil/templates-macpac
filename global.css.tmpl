/* Header Styles -----------------------------------------------------------------*/

/*no zoom in*/
* {
    touch-action: manipulation;
}

html,
body {
    background: var(--background-color);
    /*background: #124280;*/
    font-size: 16px;
    margin: 0;
    padding: 0;
    height: 100%;
    width: 100%;
    font-family: 'Roboto', sans-serif;
    font-weight: normal;
    overflow: hidden;
    overflow-y: auto;
}

article>header>h3 {
    margin: 0px;
}

#wrap {
    max-width: 1100px;
    min-height: 100%;
    position: relative;
    margin: 0 auto;
}

.nowrap {
    white-space: nowrap;
}

#maincontent {
    padding-bottom: 80px;
}

.svg-50 {
    height: 50px;
    padding: 10px;
    color: var(--primary)
}

#header {
    width: 100%;
    background: #124280;
    box-shadow: rgba(60, 64, 67, 0.3) 0px 1px 2px 0px, rgba(60, 64, 67, 0.15) 0px 2px 6px 2px;
}


.white {
    color: #ececec;
}

.white:visited {
    color: #ececec;
}

.white:hover {
    text-decoration: none;
    color: #cecaca;
    text-shadow: 0px 0px 2px rgba(255, 255, 255, 0.25);
}



.con-info {
    display: grid;
    min-height: 75px;
    /* to match contactbox*/
    grid-template-columns: auto;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
}

.header-logo {
    /* TODO wendy: new cool logo :)
    update 5/8: ty Wendy
     */
    margin: 0px 5px;
    float: left;
    min-height: 40px;
    min-width: 200px;
    max-height: 75px;

}

.header-info {
    font-size: 0.9em;
    color: #ececec;
    margin: 0px;
    padding: 0px 0px 0px 20px;
}

.header-title {
    margin: 0px;
    color: #ececec;
    font-weight: bold;
    font-size: 0.9em;
    max-height: 25px;
}

.header-subtitle {
    margin: 0px;
    color: #ececec;
    font-size: min(3.5vw, 2.5vh);
    margin: 0px;
    max-height: 20px;
}

.contactbox {
    border-radius: 3px;
    text-align: right;
    color: white;
}

.userdrop, .menudrop {
    padding: 10px;
}

.con-info, #contactbox, #menu {
    display: none;
}

/* header responsive controls */


@media (min-width: 10px) {
    #header {
    }    

    .contactbox {
        padding: 15px;
    }

    .customer-name {
        margin: 0px;
        padding: 20px 0px;
    }

    ul#toc a {
        margin: 5px;
    }

    .mobile-nav {
        color: white;
    }

    .mobile-nav > ul > li > a {
        padding: 15px 20px;
    }

    .header-info {
        display: none;
    }
    .con-info{
            grid-template-rows: 75px;
    }    
}

@media (min-width: 576px) {
    .mobile-nav {
    display: none;
    }

    #header > div {
        display: grid;
        margin: 0px auto 0px auto;
        max-width: 1100px;
        display: grid;
        grid-template-columns: 1fr 170px
    }
    .con-info {
        grid-area: 1 / 1 / 2 / 2;
        display: grid;
    }
    
    #contactbox {
        grid-area: 1 / 2 / 2 / 3;
        display: grid;
        padding: 12px 15px 12px 0px;
        background: #263b6d;
        border-radius: 7px;
        margin: 7px;
    }

    ul#toc a {
        margin: 0px;
    }

    #contactbox > a.white > b {
        font-size: 1rem;
        padding: 10px 0px;
    }

    .customer-name {
        padding: 10px 0px;
        font-size: 0.8rem;
        margin: 0px;
    }

    .low1 {
        font-size: 1rem;
        padding: 0px 0px;
        margin: 0px;
    } 

    #menu {
        grid-area: 2 / 1 / 3 / 3;
        display: block;
    }
    .header-info {
        display: block;
    }
    .con-info{
        grid-template-rows: 75px auto;
    }
}








p.update {
    font-size: 9pt;
    margin: 10px 5px 8px 5px;
    color: #cc0000;
}

/* Search Bar 
Button 
and Cart


*/


.search-wrap {
    border-radius: 4px;
    display: grid;
    grid-template-columns: auto 65px;
    grid-template-rows: 50px;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
    margin: 0px -5px;

}

form.searchbar {
    grid-area: 1 / 1 / 2 / 2;
    display: grid;
    height: 50px;
    margin: 0px 10px 0px 10px;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
    grid-template-rows: inherit;
}

form.searchbar input[type=search] {
    box-shadow: 0 0 3px #aba8a8;
    box-sizing: border-box;
    position: relative;
    left: -10px;
    background: white;
    padding: 18px;

}

form.searchbar input[type=text]:focus {
    outline: 0;
}

form.searchbar button {
    border: 0px;
    grid-area: 1 / 2 / 2 / 3;
    height: inherit;

}

/* search button resize */
@media (min-width: 100px) {
    form.searchbar {
        grid-template-columns: auto 60px;
    }

    form.searchbar button {
        font-size: 0;
    }

    button i.fa-solid {
        font-size: 15pt;
        padding: 2.5pt;
    }
}

@media (min-width: 768px) {
    form.searchbar {
        grid-template-columns: auto 120px;
    }

    form.searchbar button.button {
        font-size: 0.9em;
    }

    button i.fa-solid {
        font-size: 14pt;
        font-weight: bold;
    }
}

/*end button resize*/

.breadcrumb-header {
    font-size: 0.9em;
    margin: 0px 0px 25px 20px;
}

.breadcrumb-header::after {
    content: "\a";
    white-space: pre;
}


img.right1 {

    margin: 0 0 0 5px;
}

.cart1 {
    border-radius: 4px;
    padding: 0px 4px;
}

.cart1 a i {
    font-size: 18px;
    color: #fff;

}


.home-wrap {
    display: grid;
    grid-template-rows: auto auto;
    grid-template-columns: 2fr 1fr;
    grid-column-gap: 0px;
    grid-row-gap: 0px;

    margin-top: 20px;
}



/* Login Page */
.login-wrap {
    display: grid;
}

.login-wrap article {
    margin: 20px;
}


.login-info a {}

.login-info h3 {
    line-height: 90%;
}

@media (min-width: 100px) {
    .login-wrap {
        grid-template-columns: repeat(1, 1fr);
    }

    .login-form {}
}

@media (min-width: 768px) {
    .login-wrap {
        grid-template-columns: repeat(2, 1fr);
        margin: 0px 10px;
    }

    .login-form {}
}



/* Footer Styles -----------------------------------------------------------------*/

article#footer {
    margin: 0px;
    padding: 0px;
}

article#footer>footer {
    margin: auto;
    text-align: center;
    padding: 60px 0px;
    height: 130px;
    font-size: 0.75rem;
}

#footer nav {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
}

.footer-top {
    margin-left: auto;
}

.footer-copy {
    text-align: center;
}

#backToTop {
    display: none;
    position: fixed;
    width: 60px;
    height: 60px;
    padding: 0px;
    z-index: 99;
    font-size: 18px;
    border: none;
    outline: none;
    background-color: var(--primary);
    color: white;
    cursor: pointer;
    border-radius: 50%;
    right: 5%;
    bottom: 2%;
    box-shadow: 0 0 15px rgba(0, 0, 0, 0.4);
    transition: display 200ms;
}

#backToTop:hover {
    background-color: var(--primary-hover);
}



/* Index Page Styles ------------------------------------------------------------*/

#quicklinks {
    font-size: 10pt;
    border-radius: 3px;
    margin: 0px;
}

article#quicklinks>div.container>p {
    font-weight: bold;
    margin: 0px;
}

article#quicklinks>div.container>a.products {
    padding: -1px 10px;
}

article#quicklinks>div.container>a.products>p {
    margin: 0px;
}

.container {
    align-items: center;
    justify-content: left;

    padding: 0 3px 10px 3px;
}


/* Home images container */

.homeProducts {
    margin: 0px 0px;
    display: grid;
}

.homeimage-wrap {
    display: grid;
    overflow: hidden;
    position: relative;
    border: 0px;
    width: 100%;
    box-shadow: 0px 3px 5px rgba(0, 0, 0, 0.2);
    margin: 0px auto 0px auto;
}

.homeimage-wrap img {
    transition: transform .5s ease;
    width: 100%;
    height: 100%;
    object-fit: cover;
    border: 1px #111111;
    margin: 0px;
    overflow: hidden;
}

.homeimage-caption {
    position: absolute;
    padding: 12vh 0% 0% 5%;
    pointer-events: auto;
    width: 100%;
    height: 100%;

    background: linear-gradient(to top,
            hsla(0, 0%, 0%, 0.64) 0%,
            hsla(0, 0%, 0%, 0.632) 7.6%,
            hsla(0, 0%, 0%, 0.609) 14%,
            hsla(0, 0%, 0%, 0.574) 19.2%,
            hsla(0, 0%, 0%, 0.529) 23.7%,
            hsla(0, 0%, 0%, 0.475) 27.7%,
            hsla(0, 0%, 0%, 0.417) 31.5%,
            hsla(0, 0%, 0%, 0.354) 35.3%,
            hsla(0, 0%, 0%, 0.291) 39.4%,
            hsla(0, 0%, 0%, 0.228) 44.1%,
            hsla(0, 0%, 0%, 0.169) 49.6%,
            hsla(0, 0%, 0%, 0.115) 56.3%,
            hsla(0, 0%, 0%, 0.069) 64.4%,
            hsla(0, 0%, 0%, 0.033) 74.2%,
            hsla(0, 0%, 0%, 0.009) 86%,
            hsla(0, 0%, 0%, 0) 100%);
    text-decoration: none;
    color: white;
    overflow-wrap: normal;
    box-shadow: 0px 0px 10px -1px #797979;
}


.homeimage-wrap:hover img {
    transform: scale(1.25);
    filter: #111;
}



#custlogin {
    padding: 0px;
    margin: 0px;
    max-width: 750px;
    min-height: 300px;
    line-height: 0.5;
    background-color: #d8d8d8;
    background-image: url("images/bridge-2023.jpg");
    background-position: center;
    background-size: cover;
}

#custlogin a {
    width: 50%;
