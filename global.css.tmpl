/* Pico CSS */
:root {
  --primary: #296adb;
}

:root:not([data-theme=dark]), [data-theme=light] {
  --primary: #296adb;
  --primary-hover: #1b4388;
}

/* Header Styles -----------------------------------------------------------------*/

html,
body {
    background: #faf9f6;
    font-family: arial, sans-serif;
    font-size: 16px;
    margin: 0;
    padding: 0;
    height: 100%;
    width: 100%;
}

#wrap {
    max-width: 1100px;
    min-height: 100%;
    position: relative;
    margin: 0 auto;
}

.nowrap {
    white-space: nowrap;
}

#maincontent {
    padding-bottom: 80px;
}

#header {
    /* background: url('dist/header-bg.jpg');
    TODO wendy: new header background if picture prefered 
    */
    background: #0a3e81;
    max-width: 1100px;
    height: 110px;
    box-shadow: 0 5px 10px #000033;
    margin: 0 0 25px 0;

    display: grid;
    grid-template-columns: 3fr 1fr;
    grid-template-rows: 80px;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
}


.con-info {
    display: grid;
    grid-template-columns: auto;
    grid-template-rows: 60px 15px;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
}

.header-logo {
    /* TODO wendy: new cool logo :) */
    margin: 0px 10px;
    float:left;
    min-height: 40px;
    max-height: 75px;
    grid-area: 1 / 1 / 2 / 2;

}

.header-info {
    max-height: 15px;
    font-size: min(2vw, 1.5vh);
    color: #ececec;
    padding: 15px 0px 0px 20px;
    grid-area: 2 / 1 / 3 / 2;
}

.header-title {
    margin:0px;
    color: #ececec;
    font-weight: bold;
    font-size: min(4.5vw, 4vh);
    max-height: 25px;
}

.header-subtitle {
    margin:0px;
    color: #ececec;
    font-size: min(3.5vw, 2.5vh);
    margin:0px;
    max-height: 20px;
}

.contactbox {
    min-height:90px;
    min-width:100px;
    border: 1px solid #999;
    background: #eee;
    border-radius: 3px;
    margin: 10px 10px 10px auto;
    padding: 5px;
    text-align: right;
    grid-area: 1 / 2 / 2 / 3;
}

.contactbox p {
    font-size: 10pt;
    margin: 5px 5px 0px 5px;
}

p.update {
    font-size: 9pt;
    margin: 10px 5px 8px 5px;
