/* Header Styles -----------------------------------------------------------------*/

html,
body {
    background: #eaeefa;
    font-family: arial, sans-serif;
    font-size: 16px;
    margin: 0;
    padding: 0;
    height: 100%;
    width: 100%;
}

#wrap {
    max-width: 1100px;
    min-height: 100%;
    min-width: 600px;
    position: relative;
    margin: 0 auto;
}

.nowrap {
    white-space: nowrap;
}

#maincontent {
    padding-bottom: 80px;
}

#header {
    /* background: url('dist/header-bg.jpg');
    TODO wendy: new header background if picture prefered 
    */
    background: #0a3e81;
    max-width: 1100px;
    height: 130px;
    box-shadow: 0 5px 10px #000033;
    margin: 0 0 25px 0;

    display: grid;
    grid-template-columns: min(auto, 150px) 3fr 1fr;
    grid-template-rows: 1fr;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
    
}

.header-logo {
    grid-area:1 / 1 / 2 / 2;
    background: url('images/bridge.png') no-repeat;
    /* TODO wendy: new cool logo :) */
    margin:10px;
    background-size: contain;
    background-position: center;
    min-width:30px;
}

.con-info {
    margin: 10px 10px auto 5px;
    max-height:120px;
    grid-area: 1 / 2 / 2 / 3;
}

.header-title {
    margin:0px;
    color: #ececec;
    font-weight: bold;
    font-size: min(4.5vw, 4vh);
    max-height: 25px;
}

.header-subtitle {
    margin:0px;
    color: #ececec;
    font-size: min(3.5vw, 2.5vh);
    margin:0px;
    max-height: 20px;
}

.header-info {
    max-height: 15px;
    font-size: min(1.7vw, 1.5vh);
    color: #ececec;
}

.contactbox {
    background: #eeedec;
    min-height:90px;
    min-width:110px;
    border: 1px solid #999;
    border-radius: 3px;
    font-size: 1vw;
    margin: 10px 10px 10px auto;
    padding: 5px;
    text-align: right;
    grid-area: 1 / 3 / 2 / 4;
}

p.update {
    font-size: 9pt;
    margin: 10px 5px 8px 5px;
    color: #cc0000;
}

.search-wrap {
    display: grid;
    grid-template-columns: auto fit-content(100px);
    grid-template-rows: 50px;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
    
}

form.searchbar {
    min-width:300px;
    display: grid;
    grid-template-columns: auto 60px;
    grid-template-rows: 50px;
    grid-column-gap: 0px;
    grid-row-gap: 0px;
}

form.searchbar input[type=text] {
    margin:6px;
    padding: 6px;
    outline: none;
    border: 1px solid #999999;
    border-radius: 5px;
    background-color: #FBFBFB;
    vertical-align: top;
    font-size: 10pt;

    
    grid-area: 1 / 1 / 2 / 2;
}
form.searchbar input[type=submit] {
    margin-top: 5px;
    margin-bottom: 5px;
    width:50px;

    grid-area: 1 / 2 / 2 / 3;
}

img.right1 {
    float: right;
    margin: 0 0 0 5px;
}

.cart1 {
    float: right;
    height: 43px;
    width: 55px;
    font-size: 10pt;
    text-align: right;

    grid-area: 1 / 2 / 2 / 3;

}

