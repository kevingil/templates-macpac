    margin: 0px 20px;
    text-shadow: 0px 0px 2px rgba(0, 0, 0, 0.75);
}

#custlogin p {
    color: #e5e5e5;
    line-height: 100%;
    text-shadow: 0px 0px 2px rgba(0, 0, 0, 0.15);
}

#custlogin > div {
    backdrop-filter: blur(2px);
    background: linear-gradient(0deg, rgba(0,0,0,0.7539390756302521) 3%, rgba(0,0,0,0) 100%);
    padding: 40px 20px;
    width: 100%;
    height: 100%;
}

.blue1 {
    font-weight: bold;
    color: #e5e5e5;
    text-shadow: 0px 0px 2px rgba(0, 0, 0, 0.75);
}

.textlogin {
    line-height: 140%;
    text-shadow: 0px 0px 2px rgba(0, 0, 0, 0.75);
}

.homeText {
    padding: 2rem;
    line-height: 140%;
}

.stretch {
    width: 100%;
    display: inline-block;
    font-size: 0;
    line-height: 0
}


@media (min-width: 20px) {
    #custlogin {
        grid-area: 1 / 1 / 2 / 3;
    }

    #quicklinks {
        grid-area: 2 / 1 / 3 / 3;
    }

    .homeText {
        grid-area: 3 / 1 / 4 / 3;
    }

    .invBorder {
        grid-area: 4 / 1 / 4 / 4;
    }

    .homeProducts {
        grid-template-columns: repeat(2, 1fr);
        grid-template-rows: 170px 170px 170px;
    }

    .homeimage-caption {
        font-size: 1.5em;
        overflow: visible;
    }
}

@media (min-width: 768px) {
    #custlogin {
        grid-area: 1 / 1 / 2 / 2;
    }

    #quicklinks {
        grid-area: 1 / 2 / 2 / 3;
    }

    .homeText {
        grid-area: 2 / 1 / 3 / 3;
    }

    .invBorder {
        grid-area: 3 / 1 / 4 / 3;
    }

    .homeProducts {
        grid-template-columns: repeat(3, 1fr);
        grid-template-rows: 220px 220px;
    }

    .homeimage-caption {
        font-size: 2em;
        padding-top: 14vh;
    }
}



