}

.search-wrap {
	border-radius: 4px;
  display: grid;
  height: 50px;
  grid-template-columns: auto 65px;
  grid-template-rows: 50px;
  grid-column-gap: 0px;
  grid-row-gap: 0px;


}

form.searchbar {
  display: grid;
  margin: 0px 10px 0px 10px;
  grid-area: 1 / 1 / 2 / 2;
  grid-template-columns: auto 120px;
  grid-template-rows: 50px;
  grid-column-gap: 0px;
  grid-row-gap: 0px;
}

form.searchbar input[type=text] {
  border: 1px;
  grid-area: 1 / 1 / 2 / 2;
  height: 50px;
  box-shadow: 0 0 3px #ccc;
  padding: 12px 20px;
  box-sizing: border-box;
  border-radius: 10px;
  margin: 0px 0px 0px -10px
  
}

form.searchbar input[type=text]:focus
{
	outline: 0;
}



/* Search Bar Button */

form.searchbar button {
	border: 0px;
	background: none;
	background-color: #296adb;
	color: #f1f1f1;
  height: 50px;
  
  grid-area: 1 / 2 / 2 / 3;

}





