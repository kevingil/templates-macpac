      <TMPL_INCLUDE NAME="filters.tmpl">

          <script>
            // opens filter tags if screen over 768

            var filtersAttr = function () {
              var width = $(window).width();
              if (width < 768) {
                document.querySelector("#pagecontrols").removeAttribute('open');
                document.querySelector("#filtercontainer").removeAttribute('open');
              } else {
                document.querySelector("#pagecontrols").setAttribute('open', true);
                document.querySelector("#filtercontainer").setAttribute('open', true);
              }
            }

            $(document).ready(function () {
              filtersAttr();
            });
            $(window).resize(function () {
              filtersAttr();
            });


          </script>

